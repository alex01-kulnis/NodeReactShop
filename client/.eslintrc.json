{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": [
            "off",
            "windows"
        ],
        "require-jsdoc": 0,
        "max-len": 0,
        "object-curly-spacing": 0,
        "operator-linebreak": 0,
        "camelcase": 0,
        "react/prop-types": 0
    }
}